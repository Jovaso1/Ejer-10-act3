    private void btncalcularActionPerformed(java.awt.event.ActionEvent evt) {                                            
        // TODO add your handling code here:
        double num_insc, patrimonio, estrato, pag_matri;
        String nombre;
        
        num_insc = Double.parseDouble(txtnuminscripcion.getText());
        nombre = String.valueOf(txtnombres.getText());        
        patrimonio = Double.parseDouble(txtpatrimonio.getText());
        estrato = Double.parseDouble(txtestratosocial.getText());
        pag_matri = Matricula.calcMatricula(patrimonio, estrato);       
        
        txtnuminscripcion1.setText(String.valueOf(num_insc));
        txtnombres1.setText(nombre);
        txtpago.setText(String.valueOf(pag_matri));
    }                                           

    private void btnborrarActionPerformed(java.awt.event.ActionEvent evt) {                                          
        txtnuminscripcion.setText("");
        txtnombres.setText("");
        txtpatrimonio.setText("");
        txtestratosocial.setText("");
        txtnuminscripcion1.setText("");
        txtnombres1.setText("");
        txtpago.setText("");
        
    }                                         

    private void btnsalirActionPerformed(java.awt.event.ActionEvent evt) {                                         
        dispose();
    }                                        

