Clase:
package ejer.pkg10.act3;
public class Matricula {
    public static double calcMatricula(double patrimonio, double estrato){
    double pag_matri;
    if (patrimonio > 2000000 && estrato > 3){
        pag_matri = ((patrimonio*0.03) + 50000);
    }else{
        pag_matri = 50000;
    }
    return pag_matri;       
    }
}
